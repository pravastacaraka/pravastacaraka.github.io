    "@plaiceholder/next": "2.1.0",
    "airtable": "0.11.1",
    "framer-motion": "4.1.17",
    "next": "11.1.2",
    "plaiceholder": "2.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.17.0",
    "react-icons": "4.3.1",
    "react-markdown": "7.0.1",
    "sharp": "0.29.1"
